[hardware]
default_settings = @LIB_PATH@settings/default_settings.json
custom_settings = @LIB_PATH@settings/custom_settings.json
heads = @LIB_PATH@heads
cameras = @LIB_PATH@cameras

[socket]
host = 0.0.0.0
port = 9002

[xmlrpc]
xmlrpc_host = 0.0.0.0
xmlrpc_port = 8000

[general]
lock = @FABUI_TEMP_PATH@LOCK
macro_response = @FABUI_TEMP_PATH@macro_response
jog_response = @FABUI_TEMP_PATH@jog_response.json
temperature = @FABUI_TEMP_PATH@temperature.json
trace = @FABUI_TEMP_PATH@trace
command = @FABUI_TEMP_PATH@command
temp_path = @FABUI_TEMP_PATH@
task_monitor = @FABUI_TEMP_PATH@task_monitor.json
emergency_file = @FABUI_TEMP_PATH@emergency.json
ext_path = @SHARED_PATH@ext/
python_scripts = @PYTHON_PATH@
bigtemp_path = @BIGTEMP_PATH@
database = @LIB_PATH@fabtotum.db
autoinstall_file = @WWW_PATH@AUTOINSTALL
wizard_file = @WWW_PATH@WIZARD
restore_file = @WWW_PATH@RESTORE
uploads = @UPLOAD_PATH@
userdata_path = @USERDATA_PATH@
notify_file = @FABUI_TEMP_PATH@notify.json
settings_path = @LIB_PATH@settings/
plugins_path  = @LIB_PATH@plugins/
bundles_path  = @COLIBRI_LIB_PATH@bundle/

[monitor]
backtrack = 20
period = 5

[notify]
backtrack = 30

[jog]
backtrack = 20

[usb]
usb_disk_folder = @USB_MEDIA_PATH@
usb_file = /dev/sda1

[gpio]
pin = 2

[social]
blog_feed_url = http://www.fabtotum.com/feed/?cat%3D5%2C6
blog_feed_file = @FABUI_TEMP_PATH@blog_feed.xml
twitter_feed_url = http://www.fabtotum.com/twitter_feed.json
twitter_feed_file = @FABUI_TEMP_PATH@twitter_feed.json
instagram_feed_url = http://www.fabtotum.com/instagram.json
instagram_feed_file = @FABUI_TEMP_PATH@instagram.json

[updates]
colibri_endpoint = http://update.fabtotum.com/colibri/
firmware_endpoint = http://update.fabtotum.com/firmware/
